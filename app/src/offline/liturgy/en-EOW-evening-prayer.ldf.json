n","type":"option","condition":{"conditions":[{"preference":{"value":"before","is":false,"key":"owe"}},{"preference":{"value":"yes","is":false,"key":"ofe"}}],"mode":"and"},"hidden":false,"value":[{"type":"text","category":[],"source":{"api":"","source":"","citation":""},"metadata":{},"version_label":"Lay/Deacon","style":"prayer","value":["Almighty God have mercy on us, forgive us all our sins through the grace of Jesus Christ, strengthen us in all goodness, and by the power of the Holy Spirit keep us in eternal life."],"hidden":false},{"source":{"citation":"","source":"","api":""},"style":"prayer","metadata":{},"type":"text","category":[],"value":["Almighty God have mercy on you, forgive you all your sins through the grace of Jesus Christ, strengthen you in all goodness, and by the power of the Holy Spirit keep you in eternal life."],"version_label":"Priest","hidden":false}],"category":[],"metadata":{"selected":0,"editor_selected":0}},{"metadata":{"level":2},"hidden":false,"category":[],"type":"heading","source":{"api":"","citation":"","source":""},"style":"text","value":["Invitatory and Psalter"],"condition":{"mode":"and","conditions":[{"preference":{"key":"owe","is":false,"value":"before"}}]}},{"category":[],"value":["All stand"],"hidden":false,"source":{"citation":"","source":"","api":""},"metadata":{},"condition":{"mode":"and","conditions":[{"preference":{"value":"before","is":false,"key":"owe"}}]},"type":"rubric"},{"category":[],"style":"preces","hidden":false,"source":{"citation":"p. 20","source":"EOW 1","api":""},"condition":{"mode":"and","conditions":[{"preference":{"value":"before","is":false,"key":"owe"}}]},"value":[{"text":"O God, be not far from us.","label":"Officiant","response":""},{"label":"People","text":"Come quickly to help us, O God."}],"metadata":{},"type":"responsive"},{"hidden":false,"condition":{"conditions":[{"preference":{"is":false,"key":"owe","value":"before"}}],"mode":"and"},"style":"gloria","lookup":{"type":"slug"},"type":"refrain","slu