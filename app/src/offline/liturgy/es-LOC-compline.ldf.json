      "language": "es",
      "value": [
        {
          "value": [
            {
              "number": "1",
              "verse": "Y ahora bendigan al Señor, siervos todos del Señor, *",
              "halfverse": "los que de noche están de pie en la casa del Señor.",
              "type": "psalm-verse"
            },
            {
              "verse": "Eleven las manos hacia el santuario, y bendigan al Señor. *",
              "type": "psalm-verse",
              "number": "2",
              "halfverse": "El Señor que hizo los cielos y la tierra, te bendiga desde Sión."
            }
          ],
          "type": "psalm-section"
