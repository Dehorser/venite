          "value": [
            {
              "verse": "Open my lips, O Lord, *",
              "halfverse": "and my mouth shall proclaim your praise.",
              "number": "",
              "type": "psalm-verse"
            },
            {
              "type": "psalm-verse",
              "verse": "Create in me a clean heart, O God, *",
              "number": "",
              "halfverse": "and renew a right spirit within me."
            },
            {
              "number": "",
              "halfverse": "and take not your holy Spirit from me.",
              "type": "psalm-verse",
              "verse": "Cast me not away from your presence *"
            },
            {
              "number": "",
              "halfverse": "and sustain me with your bountiful Spirit.",
              "type": "psalm-verse",
              "verse": "Give me the joy of your saving help again *"
            }
          ],
          "type": "psalm-section"
        }
