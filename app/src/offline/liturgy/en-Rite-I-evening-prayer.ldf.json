":{"category":["Bible Reading Introduction"],"language":"en","value":["A Reading from ${longName}."],"sharing":{"status":"published","privacy":"public","organization":"venite"},"style":"text","type":"text"}},"hidden":false,"condition":{"mode":"and","conditions":[{"preference":{"key":"readingC","is":false,"value":"none"}}]},"lookup":{"type":"lectionary","item":{"preference":"readingC"},"table":{"preference":"lectionary"}},"version":{"preference":"bibleVersion"},"style":"long"},{"type":"responsive","hidden":false,"condition":{"mode":"and","conditions":[{"preference":{"key":"readingC","value":"none","is":false}}]},"value":[{"label":"","text":"The Word of the Lord.","response":"Thanks be to God."}],"style":"responsive"},{"category":[],"source":{"api":"","citation":"","source":""},"style":"text","condition":{"conditions":[{"preference":{"key":"sermon","value":"after_readings","is":true}}],"mode":"and"},"value":["The Sermon"],"metadata":{"level":3},"hidden":false,"type":"heading"},{"style":"text","source":{"citation":"p. 66","source":"bcp1979","api":""},"response":"Amen.","type":"text","label":"The Apostles’ Creed","hidden":false,"metadata":{"omit_response":false,"response":"Amen.","rollup":false},"display_format":"unison","value":["I believe in God, the Father almighty,\n\tmaker of heaven and earth;\nAnd in Jesus Christ his only Son our Lord;\n\twho was conceived by the Holy Ghost,\n\tborn of the Virgin Mary,\n\tsuffered under Pontius Pilate,\n\twas crucified, dead, and buried.\n\tHe descended into hell.\n\tThe third day he rose again from the dead.\n\tHe ascended into heaven,\n\tand sitteth on the right hand of God the Father almighty.\n\tFrom thence he shall come to judge the quick and the dead.\nI believe in the Holy Ghost,\n\tthe holy catholic Church,\n\tthe communion of saints,\n\tthe forgiveness of sins,\n\tthe resurrection of the body,\n\tand the life everlasting."],"category":[]},{"hidden":false,"style":"text","category":[],"type":"heading","value":["The Prayers"],"metadata":{"level":2},"source":{"source":"","api":"","citation":""}},{"style":"preces","value":[{"label":"Officiant","text":"The Lord be with you.","response":""},{"label":"People","text":"And with thy spirit."},{"label":"Officiant","text":"Let us pray."}],"hidden":false,"type":"responsive"},{"type":"text","label":"The Lord’s Prayer","display_format":"unison","value":["Our Father, who art in heaven,\n\thallowed be thy Name,\n\tthy kingdom come,\n\tthy will be done,\n\ton earth as it is in heaven.\nGive us this day our daily bread.\nAnd forgive us our trespasses,\n\tas we forgive those\n\twho trespass against us.\nAnd lead us not into temptation,\n\tbut deliver us from evil.\nFor thine is the kingdom,\n\tand the power, and the glory,\n\tfor ever and ever."],"source":{"citation":"","api":"","source":""},"style":"prayer","hidden":false,"metadata":{"rollup":false,"omit_response":false}},{"metadata":{"editor_selected":0,"selected":0},"type":"option","hidden":false,"source":{"source":"","citation":"","api":""},"slug":"suffrages_ep","value":[{"hidden":false,"label":"Suffrages A","metadata":{},"category":[],"style":"preces","value":[{"label":"V.","text":"O Lord, show thy mercy upon us;","response":""},{"text":"And grant us thy salvation.","label":"R."},{"label":"V.","text":"Endue thy ministers with righteousness;"},{"label":"R.","text":"And make thy chosen people joyful."},{"label":"V.","text":"Give peace, O Lord, in all the world;"},{"text":"For only in thee can we live in safety.","label":"R."},{"label":"V.","text":"Lord, keep this nation under thy care;"},{"label":"R.","text":"And guide us in the way of justice and truth."},{"label":"V.","text":"Let thy way be known upon earth;"},{"label":"R.","text":"Thy saving health among all nations."},{"label":"V.","text":"Let not the needy, O Lord, be forgotten;"},{"label":"R.","text":"Nor the hope of the poor be taken away."},{"label":"V.","text":"Create in us clean hearts, O God;"},{"label":"R.","text":"And sustain us with thy Holy Spirit."}],"source":{"source":"","api":"","citation":""},"type":"responsive"},{"label":"Suffrages B","value":[{"text":"That this evening may be holy, good, and peaceful,"},{"text":"That thy holy angels may lead us in paths of peace and goodwill,","response":"We entreat thee, O Lord."},{"text":"That we may be pardoned and forgiven for our sins and offenses,","response":"We entreat thee, O Lord."},{"text":"That there may be peace to thy Church and to the whole world,","response":"We entreat thee, O Lord."},{"response":"We entreat thee, O Lord.","text":"That we may depart this life in thy faith and fear, and not be condemned before the great judgment seat of Christ,"},{"text":"That we may be bound together by thy Holy Spirit in the communion of [___________ and] all thy saints, entrusting one another and all our life to Christ,","response":"We entreat thee, O Lord."}],"metadata":{"response":"We entreat thee, O Lord."},"source":{"citation":"","source":"","api":""},"style":"litany","hidden":false,"category":[],"type":"responsive"}],"category":[]},{"language":"en","type":"text","source":{"citation":"","source":"","api":""},"version":"rite_i","hidden":false,"category":[],"metadata":{"omit_response":false,"omit_label":false,"rollup":false},"label":"The Collect of the Day","lookup":{"rotate":false,"type":"collect","allow_multiple":true},"style":"prayer"},{"lookup":{"rotate":true,"filter":"—","type":"slug"},"style":"prayer","condition":{"mode":"and","conditions":[{"weekday":{"except":[],"only":["Monday","Tuesday","Wednesday","Thursday"]}}]},"value":[""],"type":"text","version":"rite_i","metadata":{"omit_response":false,"rollup":false},"category":[],"hidden":false,"source":{"api":"","source":"","citation":""},"slug":"ep-daily-collect","language":"en"},{"style":"prayer","condition":{"conditions":[{"weekday":{"except":[],"only":["Friday"]}}],"mode":"and"},"source":{"api":"","source":"","citation":""},"hidden":false,"metadata":{},"label":"A Collect for Fridays","type":"text","value":["O Lord Jesus Christ, who by thy death didst take away the sting of death: Grant unto us thy servants so to follow in faith where thou hast led the way, that we may at length fall asleep peacefully in thee, and awake up after thy likeness; for thy tender mercies’ sake."],"category":[]},{"hidden":false,"category":[],"label":"A Collect for Saturdays","source":{"api":"","source":"","citation":""},"metadata":{},"style":"prayer","condition":{"mode":"and","conditions":[{"weekday":{"only":["Saturday"],"except":[]}}]},"value":["O God, the source of eternal light: Shed forth thine unending day upon us who watch for thee, that our lips may praise thee, our lives may bless thee, and our worship on the morrow may give thee glory; through Jesus Christ our Lord. "],"type":"text"},{"source":{"citation":"","source":"","api":""},"label":"A Collect for Sundays","metadata":{},"category":[],"condition":{"conditions":[{"weekday":{"only":["Sunday"],"except":[]}}],"mode":"and"},"value":["Lord God, whose Son our Savior Jesus Christ triumphed over the powers of death and prepared for us our place in the new Jerusalem: Grant that we, who have this day given thanks for his resurrection, may praise thee in that City of which he is the light; and where he liveth and reigneth for ever and ever. "],"type":"text","style":"prayer","hidden":false},{"slug":"ep_prayers_for_mission","version":"rite_i","source":{"citation":"","source":"","api":""},"language":"en","style":"prayer","metadata":{"omit_response":false,"rollup":false},"hidden":false,"lookup":{"rotate":true,"type":"slug"},"category":[],"type":"text","value":[""]},{"metadata":{"level":3},"type":"heading","style":"text","category":[],"condition":{"conditions":[{"preference":{"value":"after_collects","key":"sermon","is":true}}],"mode":"and"},"value":["The Sermon"],"source":{"source":"","citation":"","api":""},"hidden":false},{"hidden":false,"value":["Here may be sung a hymn or anthem.","Authorized intercessions and thanksgivings may follow."],"type":"rubric"},{"hidden":false,"style":"authorized-prayers","type":"text"},{"type":"rubric","hidden":false,"value":["Before the close of the Office one or both of the following may be used"]},{"category":[],"style":"prayer","value":["Almighty God, Father of all mercies,\nwe thine unworthy servants\ndo give thee most humble and hearty thanks\nfor all thy goodness and loving-kindness\nto us and to all men.\nWe bless thee for our creation, preservation,\nand all the blessings of this life;\nbut above all for thine inestimable love\nin the redemption of the world by our Lord Jesus Christ;\nfor the means of grace, and for the hope of glory.","And, we beseech thee,\ngive us that due sense of all thy mercies,\nthat our hearts may be unfeignedly thankful;\nand that we show forth thy praise,\nnot only with our lips, but in our lives,\nby giving up our selves to thy service,\nand by walking before thee\nin holiness and righteousness all our days;\nthrough Jesus Christ our Lord,\nto whom, with thee and the Holy Ghost,\nbe all honor and glory, world without end. "],"type":"text","metadata":{"rollup":false,"omit_response":false},"label":"The General Thanksgiving","hidden":false,"source":{"api":"","citation":"p. 71","source":"bcp1979"}},{"value":["Almighty God, who hast given us grace at this time with one accord to make our common supplication unto thee; and hast promised through thy well-beloved Son that when two or three are gathered together in his Name thou wilt be in the midst of them: Fulfill now, O Lord, the desires and petitions of thy servants as may be best for us; granting us in this world knowledge of thy truth, and in the world to come life everlasting."],"source":{"citation":"p. 72","source":"bcp1979","api":""},"type":"text","label":"A Prayer of St. Chrysostom","hidden":false,"metadata":{"omit_response":false,"rollup":false},"category":[],"style":"prayer"},{"value":["Then may be said"],"hidden":false,"type":"rubric"},{"condition":{"mode":"and","conditions":[{"season":{"except":["Easter","Ascension","Pentecost"],"only":[]}}]},"type":"responsive","value":[{"response":"Thanks be to God.","label":"","text":"Let us bless the Lord."}],"style":"responsive","hidden":false},{"hidden":false,"style":"responsive","type":"responsive","condition":{"mode":"and","conditions":[{"season":{"except":[],"only":["Easter","Ascension","Pentecost"]}}]},"value":[{"label":"","response":"Thanks be to God. Alleluia, alleluia..","text":"Let us bless the Lord. Alleluia, alleluia."}]},{"type":"rubric","hidden":false,"value":["The Officiant may then conclude with one of the following"]},{"hidden":false,"lookup":{"rotate":true,"type":"category"},"style":"short","category":["Closing Sentence"],"type":"bible-reading"},{"style":"text","value":["The Sermon"],"category":[],"metadata":{"omit_response":false,"level":3},"type":"heading","hidden":false,"condition":{"conditions":[{"preference":{"is":true,"key":"sermon","value":"after_office"}}],"mode":"and"},"source":{"source":"","citation":"","api":""}},{"sharing":{"collaborators":[],"status":"published","privacy":"public","owner":"ikvC2kTwM0MhmiqfMOi2fFZynJr2","organization":"venite"},"version":"Rite-I","hidden":false,"type":"liturgy","category":[],"slug":"the-angelus","lastRevision":61,"label":"The Angelus","source":{"source":"","api":"","citation":""},"lookup":{"rotate":false,"type":"slug"},"metadata":{"evening":false,"liturgyversions":[]},"condition":{"conditions":[{"preference":{"key":"angelus","is":true,"value":"after"}}],"mode":"and"},"language":"en"},{"condition":{"conditions":[{"preference":{"is":true,"value":"after","key":"andrewes"}}],"mode":"and"},"category":[],"label":"The Angelus","hidden":false,"language":"en","sharing":{"organization":"venite","privacy":"public","owner":"ikvC2kTwM0MhmiqfMOi2fFZynJr2","status":"published","collaborators":[]},"version":"Rite-I","slug":"andrewes-evening","lastRevision":61,"metadata":{"evening":false,"supplement":false,"liturgyversions":[]},"source":{"api":"","so