          "version_label": "Love and Serve",
          "source": { "api": "", "citation": "", "source": "" },
          "style": "preces",
          "value": [
            {
              "text": "Go in peace to love and serve the Lord.",
              "response": "",
              "label": ""
            },
            { "text": "Thanks be to God.", "label": "People" }
          ],
          "metadata": {},
          "type": "responsive",
          "category": []
        },
        {
          "source": { "citation": "", "source": "", "api": "" },
          "type": "responsive",
          "value": [
            {
              "response": "",
              "text": "Let us go forth into the world, rejoicing in the power of the Spirit.",
              "label": ""
            },
            { "label": "People", "text": "Thanks be to God." }
          ],
          "metadata": {},
          "category": [],
          "hidden": false,
          "version_label": "Rejoicing in the Power",
          "style": "preces"
        },
        {
          "source": { "source": "", "api": "", "citation": "" },
          "type": "responsive",
          "value": [
            { "label": "", "response": "", "text": "Let us bless the Lord." },
            { "text": "Thanks be to God.", "label": "People" }
          ],
          "category": [],
          "metadata": {},
          "hidden": false,
          "version_label": "Bless the Lord",
          "style": "preces"
        }
      ],
