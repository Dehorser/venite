{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2019",
      "es2018",
      "dom"
    ],
    "paths": {
      "reminders": [
        "dist/reminders/reminders",
        "dist/reminders"
      ],
      "interfaces": [
        "dist/interfaces/interfaces",
        "dist/interfaces"
      ],
      "service-api": [
        "dist/service-api/service-api",
        "dist/service-api"
      ],
      "localstorage": [
        "dist/localstorage/localstorage",
        "dist/localstorage"
      ],
      "darkmode": [
        "dist/darkmode/darkmode",
        "dist/darkmode"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}