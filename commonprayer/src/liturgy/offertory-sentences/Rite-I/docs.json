      "value": [
        {
          "text": "Worthy art thou, O Lord our God, to receive glory and honor and power; for thou hast created all things, and by thy will they were created and have their being. "
        }
      ],
      "category": ["Offertory Sentences"],
      "hidden": false,
      "citation": "Revelation 4:11",
      "language": "en",
      "version_label": null,
