            {
              "response": "",
              "label": "Officiant",
              "text": "Lord, have mercy."
            },
            {
              "label": "People",
              "text": "Christ, have mercy."
            },
            {
              "text": "Lord, have mercy.",
              "label": "Officiant"
            }
          ],
